{
    "routing": {
        "routes": {
            "plain/login": {
                "origin": "null,plain,error",
                "oml": "login",
                "scope": "public"
            },
            "main/desktop": {
                "origin": "null,plain,error",
                "oml": "desktop",
                "scope": "public"
            }
        },
        "options": {
            "history": true,
            "bookmark": true,
            "cache": false,
            "jail": false,
            "eval": false
        },
        "cache": [],
        "errors": {}
    },
    "vapps": {
        "plain:Stages.Plain": {
            "use": true,
            "anim": "swap flip",
            "home": "login",
            "placeholder": "default",
            "scope": "public",
            "theater": false
        },
        "main:Stages.Main": {
            "use": false,
            "anim": "shuffle",
            "template": "Main",
            "home": "desktop",
            "placeholder": ".view",
            "scope": "public",
            "theater": false,
            "node:OML": "main_stage"
        }
    },
    "preload": {
        "/snd": [],
        "/img": [
            "bg.jpg",
            "logo.svg",
            "logo_192.png",
            "logo_48.png",
            "logo_96.png"
        ],
        "themes/io-globules-ogx/base/fonts": [
            "Roboto-Black.ttf",
            "Roboto-BlackItalic.ttf",
            "Roboto-Bold.ttf",
            "Roboto-BoldItalic.ttf",
            "Roboto-Italic.ttf",
            "Roboto-Light.ttf",
            "Roboto-LightItalic.ttf",
            "Roboto-Medium.ttf",
            "Roboto-MediumItalic.ttf",
            "Roboto-Regular.ttf",
            "Roboto-Thin.ttf",
            "Roboto-ThinItalic.ttf"
        ],
        "/json": [
            "desktop_context.json",
            "extensions.json",
            "menu_icons.json",
            "menu_pins.json",
            "programs.json",
            "sketch_brushes.json",
            "sketch_colors.json",
            "swatches.json"
        ],
        "/oml": [
            "desktop.oml",
            "display_preferences.oml",
            "display_settings.oml",
            "drives.oml",
            "file_explorer.oml",
            "login.oml",
            "main_menu.oml",
            "main_stage.oml",
            "sketch.oml",
            "task_manager.oml"
        ],
        "/html": [
            "template.Browser.html",
            "template.Calculator.html",
            "template.Clock.html",
            "template.ContextItem.html",
            "template.ContextLine.html",
            "template.ContextMenu.html",
            "template.Desktop.html",
            "template.DisplayPreferences.html",
            "template.DisplaySettings.html",
            "template.Drive.html",
            "template.Drives.html",
            "template.File.html",
            "template.FileExplorer.html",
            "template.Folder.html",
            "template.ImageViewer.html",
            "template.Login.html",
            "template.Main.html",
            "template.MainMenu.html",
            "template.MenuLetter.html",
            "template.MenuProgram.html",
            "template.Settings.html",
            "template.Sketch.html",
            "template.SketchBrush.html",
            "template.TaskManager.html",
            "template.TextEditor.html",
            "template.WebApp.html",
            "template.Camera.html"
        ]
    },
    "popups": {
        "width": 300,
        "height": 400,
        "anim": "fade scale"
    },
    "windows": {
        "head": "back",
        "width": "100%"
    },
    "views": {},
    "overlays": {},
    "splash": {
        "enabled": true,
        "image": "/img/logo.svg",
        "color": "#2d5cb4",
        "progess": true,
        "auto_hide": true,
        "hide_timeout": 1000
    },
    "rendertime": false,
    "scope": "public",
    "jwt": false
}